agment key={id} >\r\n              <li>\r\n                <Button\r\n                  classtitle=\"delete-btn\"\r\n                  color=\"danger\"\r\n                  size=\"sm\"\r\n                  onClick={() => { this.deletItem(id) }}>\r\n                  X</Button>\r\n                {title}\r\n              </li>\r\n            </Fragment>\r\n          ))}\r\n        </ul>\r\n        {/* <Button onClick={this.deletAll}> Delete All</Button> */}\r\n      </Container>\r\n    )\r\n  }\r\n}\r\n\r\nexport default TodoListe\r\n","C:\\Users\\Youcode\\Desktop\\TodoList\\client\\src\\components\\Test.js",[],{"ruleId":"32","severity":1,"message":"33","line":2,"column":21,"nodeType":"34","messageId":"35","endLine":2,"endColumn":30},{"ruleId":"32","severity":1,"message":"36","line":2,"column":32,"nodeType":"34","messageId":"35","endLine":2,"endColumn":45},{"ruleId":"32","severity":1,"message":"37","line":3,"column":10,"nodeType":"34","messageId":"35","endLine":3,"endColumn":23},{"ruleId":"32","severity":1,"message":"38","line":3,"column":25,"nodeType":"34","messageId":"35","endLine":3,"endColumn":40},{"ruleId":"32","severity":1,"message":"39","line":4,"column":16,"nodeType":"34","messageId":"35","endLine":4,"endColumn":20},"no-unused-vars","'ListGroup' is defined but never used.","Identifier","unusedVar","'ListGroupItem' is defined but never used.","'CSSTransition' is defined but never used.","'TransitionGroup' is defined but never used.","'uuid' is defined but never used."]